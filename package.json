{
	"name": "michigan-bike-maps-official",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "npm run wasm && vite dev",
		"build": "npm run wasm && vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check .",
		"modulize-wasm": "node ./gpx-file-processor-wasm/modulize.js",
		"wasm": "wasm-pack build ./gpx-file-processor-wasm --target web  --out-dir ../static/wasm && npm run modulize-wasm",
		"modulize-wasm:netlify": "node ./gpx-file-processor-wasm/modulize.js netlify",
		"wasm:netlify": "cd gpx-file-processor-wasm && wasm-pack build --target web --release && cd .. && npm run modulize-wasm:netlify"
	},
	"devDependencies": {
		"@iconify/svelte": "^5.0.1",
		"@internationalized/date": "^3.8.1",
		"@lucide/svelte": "^0.515.0",
		"@sveltejs/adapter-netlify": "^5.2.1",
		"@sveltejs/enhanced-img": "^0.8.0",
		"@sveltejs/kit": "^2.31.1",
		"@sveltejs/vite-plugin-svelte": "^6.1.2",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"@tailwindcss/vite": "^4.1.12",
		"@types/geojson": "^7946.0.16",
		"@types/leaflet": "^1.9.20",
		"@types/node": "^24.3.0",
		"bits-ui": "^2.9.3",
		"clsx": "^2.1.1",
		"formsnap": "^2.0.1",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"rollup-plugin-visualizer": "^6.0.3",
		"svelte": "^5.38.1",
		"svelte-check": "^4.3.1",
		"sveltekit-superforms": "^2.27.1",
		"tailwind-merge": "^3.3.1",
		"tailwind-variants": "^2.1.0",
		"tailwindcss": "^4.1.12",
		"tw-animate-css": "^1.3.7",
		"typescript": "^5.9.2",
		"vaul-svelte": "^1.0.0-next.7",
		"vite": "^7.1.2"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.864.0",
		"@aws-sdk/s3-request-presigner": "^3.864.0",
		"@mapbox/togeojson": "^0.16.2",
		"@maptiler/client": "^2.5.0",
		"@maptiler/leaflet-maptilersdk": "^4.1.0",
		"@maptiler/sdk": "^3.7.0",
		"@poppanator/sveltekit-svg": "^5.0.1",
		"@qwik.dev/partytown": "^0.11.2",
		"@wasm-tool/rollup-plugin-rust": "^3.0.5",
		"gpxparser": "^3.0.8",
		"leaflet": "^1.9.4",
		"leaflet-edgebuffer": "^1.0.7",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"sveltekit-rate-limiter": "^0.7.0",
		"uuid": "^11.1.0",
		"vite-plugin-wasm-pack": "^0.1.12"
	}
}
